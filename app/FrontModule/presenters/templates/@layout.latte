<body class="{block bodyClass}{/block}">

{control login}

<div class="navbar navbar-tshop navbar-fixed-top megamenu" role="navigation">

    <div class="white-background navbar-top">
        <div class="container">
            <div class="navbar-header col-sm-7">
                {*<button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse"><span*}
                {*class="sr-only"> Toggle navigation </span>*}
                {*<span class="icon-bar"> </span>*}
                {*<span*}
                {*class="icon-bar"> </span>*}
                {*<span class="icon-bar"> </span>*}
                {*</button>*}

                <button class="menu-button hidden-lg navbar-toggle" data-toggle="collapse"
                        data-target=".navbar-collapse" id="open-button">
                    <span>Menu</span>
                </button>

                {*<a href="{plink :Front:Cart:default}" class="navbar-toggle" title="Košík">*}
                {*<i class="fa fa-shopping-cart colorWhite"> </i>*}
                {*<span class="cartRespons colorWhite"> Košík </span>*}
                {*</a>*}


                <a class="navbar-brand" href="{plink :Front:Homepage:default}" title="Aquacity Slovakia">
                    <img class="img-responsive" src="{$basePath}/assets/frontend/img/super-zoo-chovatelske-potreby.svg"
                         alt="Aquacity Slovakia">
                </a>

                {*<div class="search-box pull-right hidden-lg hidden-md hidden-sm">*}
                {*<div class="input-group">*}
                {*<button class="btn btn-nobg getFullSearch" type="button"><i class="fa fa-search"> </i></button>*}
                {*</div>*}
                {*</div>*}
            </div>


            <div class="navbar-login col-sm-5">

                <div class="navbar-tel  hidden-xs">
                    <div class="phone-icon"><span class="fa fa-phone"></span></div>
                    <span><span class="navbar-tel--title">Zavolajte nám</span> <br> Tel: <span> 0123 456 789</span></span>
                </div>

                <div class="btn-group " role="group">


                    <a href="{plink :Front:Cart:default}" class="btn hidden-md hidden-lg" title="Košík">
                        <i class="fa fa-shopping-cart hide visible-xs "></i>
                        <span class="hidden-sm">Košík</span>

                    </a>

                    <a n:if="$user->isLoggedIn()" href="{plink :Front:Profile:default}" class="btn btn-success"
                                                  title="Môj účet">
                        <i class="fa fa-user "></i>
                        <span>Môj účet</span>
                    </a>

                    <a n:if="!$user->isLoggedIn()" href="#" class="btn " data-toggle="modal" data-target="#ModalLogin"
                                                   title="Prihlásenie">
                        <i class="fa fa-sign-in"></i>
                        <span class="hidden-sm"> Prihlásenie</span>
                    </a>

                    <a n:if="!$user->isLoggedIn()" href="{plink :Front:Registration:default}"
                                                   class="btn btn-default btn-signup" title="Registrácia">
                        <i class="fa fa-user-plus"></i>
                        <span class="hidden-sm"> Registrácia</span>
                    </a>

                    <a n:if="$user->isLoggedIn()" href="{plink :Front:Homepage:logout}" class="btn btn-default"
                                                  title="Odhlásiť">
                        <i class="fa fa-sign-out"></i>
                        Odhlásiť
                    </a>
                </div>
            </div>


        </div>
    </div>

    <div class="main-menu">
        <div class="container">
            <div class="navbar-collapse collapse">

                {include nav.latte}

                <div class="nav navbar-nav navbar-right hidden-xs">

                    {control cart}

                    <div class="search-box">
                        <div class="input-group">
                            <button class="btn btn-nobg getFullSearch" type="button"><i class="fa fa-search"> </i>
                            </button>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>

    <div class="container">
        <div class="search-full text-right"><a class="pull-right search-close"> <i class=" fa fa-times-circle"> </i>
            </a>

            <div class="searchInputBox pull-right">
                {form productSearchForm id=>"frm-productSearchForm2"}
                    {input query id=>"frm-productSearchForm-query2",type=>"search",class=>"search-input",placeholder=>$presenter->context->getService('translator')->translate('vyhľadávanie')}

                    <button name="btn" class="btn-nobg search-btn" type="submit"><i class="fa fa-search"> </i></button>
                {/form}
            </div>
        </div>
    </div>
</div>

{ifCurrent :Front:Homepage:*}
    <div class="header-offset">
        {include content}
    </div>
{else}
    <div class="container main-container header-offset">
        {include flashMessage.latte}
        {include content}
    </div>
{/ifCurrent}

{include footer.latte}


